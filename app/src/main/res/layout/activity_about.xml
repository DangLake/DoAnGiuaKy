<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".About">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#6750a4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Vũ Thị Hương Giang"
        android:textSize="12pt"
        android:textStyle="bold"
        android:gravity="center"
        android:id="@+id/tvTen"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/tvMaSV"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="DH52110848"
        android:textSize="12pt"
        android:textStyle="bold"
        android:gravity="center"
        android:id="@+id/tvMaSV"
        app:layout_constraintTop_toBottomOf="@+id/tvTen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/tvEmail"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="vthgiang181003@gmail.com"
        android:textSize="12pt"
        android:textStyle="bold"
        android:gravity="center"
        android:id="@+id/tvEmail"
        app:layout_constraintTop_toBottomOf="@+id/tvMaSV"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/btnSDT"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="0706827751"
        android:backgroundTint="@android:color/transparent"
        android:textColor="#000000"
        android:textStyle="bold"
        android:textSize="11pt"
        app:layout_constraintTop_toBottomOf="@+id/tvEmail"
        android:id="@+id/btnSDT"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/map"
        app:layout_constraintStart_toStartOf="parent"/>


    <fragment
        xmlns:map="http://schemas.android.com/apk/res-auto"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="0pt"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintTop_toBottomOf="@+id/btnSDT"
        map:layout_constraintBottom_toBottomOf="parent"
        android:layout_height="0pt" />

</androidx.constraintlayout.widget.ConstraintLayout>